{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "CommonJS",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target":"ES6",
    "lib": [
      "es2016",
      "dom",
      "es5"
  ]
  }
}